{
  "name": "astro-start-project",
  "description": "Astro Start Project",
  "type": "module",
  "version": "0.0.1",
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=20.15.0",
    "pnpm": ">=9.4.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "eslint": "eslint ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:fix": "eslint --fix ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:precommit": "eslint --max-warnings 0 --ignore-path .gitignore",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "format:precommit": "prettier --check . --plugin=prettier-plugin-astro",
    "stylelint": "stylelint \"./src/**/*.{css,scss,astro}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss,astro}\"",
    "stylelint:precommit": "stylelint --max-warnings 0 --ignore-path .gitignore"
  },
  "devDependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "astro": "^4.11.3",
    "astro-compress": "^2.2.28",
    "astro-critters": "^2.1.10",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.6",
    "sharp": "^0.33.4",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9.6.0"
      }
    }
  }
}
