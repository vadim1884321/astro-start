{
  "name": "astro-start-project",
  "description": "Astro Start Project",
  "type": "module",
  "version": "0.0.1",
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=22.13.1",
    "pnpm": ">=9.15.4"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "eslint": "eslint ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:fix": "eslint --fix ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:precommit": "eslint --max-warnings 0 --ignore-path .gitignore",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "format:precommit": "prettier --check . --plugin=prettier-plugin-astro",
    "stylelint": "stylelint \"./src/**/*.{css,scss,astro}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss,astro}\"",
    "stylelint:precommit": "stylelint --max-warnings 0 --ignore-path .gitignore"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.8",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@playform/compress": "^0.1.7",
    "@playform/inline": "^0.1.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "astro": "^5.2.5",
    "eslint": "^9.19.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.83.4",
    "sharp": "^0.33.5",
    "stylelint": "^16.14.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.0",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
